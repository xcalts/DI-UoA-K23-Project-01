{
  "configurations": [
    {
      "name": "MRNG",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/${fileBasenameNoExtension}",
      "args": [
        "-i",
        "${workspaceFolder}/data/train-images.idx3-ubyte",
        "-q",
        "${workspaceFolder}/data/t10k-images.idx3-ubyte",
        "-o",
        "${workspaceFolder}/results.txt",
        "-I",
        "6",
        "-N",
        "4"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++ build active file",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ],
  "version": "2.0.0"
}
